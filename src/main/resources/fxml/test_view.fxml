<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8"
            fx:controller="me.academeg.controller.TestViewController">
    <children>

        <GridPane gridLinesVisible="false" hgap="10" vgap="10" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0"
                  AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints vgrow="SOMETIMES"/>
            </rowConstraints>


            <TableView layoutX="-40.0" layoutY="52.0" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="0"
                       GridPane.halignment="CENTER" GridPane.rowIndex="0" fx:id="tableView">
                <columns>
                    <TableColumn prefWidth="75.0" text="ID" fx:id="id"/>
                    <TableColumn prefWidth="75.0" text="Фамилия" fx:id="surname"/>
                    <TableColumn prefWidth="75.0" text="Телефон" fx:id="phone"/>
                </columns>
            </TableView>
        </GridPane>
    </children>
</AnchorPane>
